<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/pp"
        android:label="@string/app_name"
        android:roundIcon="@drawable/pp"
        android:supportsRtl="true"
        android:theme="@style/Theme.New"
        tools:targetApi="31">
        <activity
            android:name=".LoginActivity"
            android:exported="false"
            android:label="@string/title_activity_login"
            android:theme="@style/Theme.New" />
        <activity
            android:name=".AssigmentActivity2"
            android:exported="false"
            android:label="@string/title_activity_assigment2"
            android:theme="@style/Theme.New" />
        <activity
            android:name=".ImageActivity2"
            android:exported="false"
            android:label="@string/title_activity_image2"
            android:theme="@style/Theme.New" />
        <activity
            android:name=".AboutActivity2"
            android:exported="false"
            android:label="@string/title_activity_about2"
            android:theme="@style/Theme.New" />
        <activity
            android:name=".InputActivity2"
            android:exported="false"
            android:label="@string/title_activity_input2"
            android:theme="@style/Theme.New" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.New">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>